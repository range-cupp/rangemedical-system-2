    <!-- Header -->
    <header>
        <div class="container">
            <a href="https://range-medical.com" class="logo">
                <img src="https://storage.googleapis.com/msgsndr/WICdvbXmTjQORW6GiHWW/media/695fe7ca6eabe6386b2d84e1.png" alt="Range Medical">
            </a>
